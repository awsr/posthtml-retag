{
  "name": "posthtml-retag",
  "version": "2.0.0-alpha1",
  "description": "Tag converter plugin for PostHTML",
  "main": "lib/index.js",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc && prettier --write ./lib",
    "test": "node --test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/awsr/posthtml-retag.git"
  },
  "keywords": [
    "html",
    "posthtml",
    "posthtml-plugin"
  ],
  "author": "Alex Wiser",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/awsr/posthtml-retag/issues"
  },
  "homepage": "https://github.com/awsr/posthtml-retag#readme",
  "peerDependencies": {
    "posthtml": "^0.16.6"
  },
  "devDependencies": {
    "posthtml": "^0.16.6",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "none"
  }
}
